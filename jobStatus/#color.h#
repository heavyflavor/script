TString ColorRed    = "\033[31m";
TString ColorGreen  = "\033[32m";
TString ColorYellow = "\033[33m";
TString ColorBlue   = "\033[34m";
TString ColorPink   = "\033[35m";
TString ColorCyan   = "\033[36m";

TString ColorBGRed    = "\033[41m";
TString ColorBGGreen  = "\033[42m";
TString ColorBGYellow = "\033[43m";
TString ColorBGBlue   = "\033[44m";
TString ColorBGPink   = "\033[45m";
TString ColorBGCyan   = "\033[46m";

TString ColorEnd = "\033[0m";
